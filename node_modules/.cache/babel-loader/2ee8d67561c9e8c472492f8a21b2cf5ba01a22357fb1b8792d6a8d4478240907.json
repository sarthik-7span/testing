{"ast":null,"code":"import TheButton from \"./TheButton.vue\";\nimport TheMin from \"./TheMin.vue\";\nexport default {\n  components: {\n    TheButton,\n    TheMin\n  },\n  name: \"ThirdIndex\",\n  data() {\n    return {\n      heading: false,\n      isActive: true,\n      html: \"<p>HTML text here</p>\",\n      text1: \"good morning\",\n      text2: \"good night\",\n      input1: null,\n      width: 100,\n      disabled: false,\n      reduce: 15,\n      superp: true,\n      count: 0,\n      hide: true,\n      redc: false\n    };\n  },\n  props: {\n    propty: Array\n  },\n  methods: {\n    gm() {\n      this.text1 = this.text2;\n    },\n    punch() {\n      this.count++;\n      console.log(this.count);\n      if (this.count % 3 == 0) {\n        this.superp = false;\n      }\n      if (this.width >= this.reduce) {\n        this.width -= this.reduce;\n      } else if (this.width <= this.reduce) {\n        this.width = 0;\n        this.disabled = true;\n      }\n    },\n    heal() {\n      this.width = 100;\n      if (this.width >= 0) {\n        this.disabled = false;\n      }\n    },\n    superpunch() {\n      this.count = 0;\n      this.width = 0;\n      this.superp = true;\n      if (this.width <= this.reduce) {\n        this.width = 0;\n        this.disabled = true;\n      }\n    },\n    submit() {\n      console.log(this.$refs);\n    }\n  }\n};","map":{"version":3,"mappings":"AA0CA;AACA;AACA;EACAA;IACAC;IACAC;EACA;EACAC;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;MACAC;MACA;QACA;MACA;MACA;QACA;MACA,OACA;QACA;QACA;MACA;IACA;IACAC;MACA;MACA;QACA;MACA;IACA;IACAC;MACA;MACA;MACA;MACA;QACA;QACA;MACA;IACA;IACAC;MACAH;IACA;EACA;AACA","names":["components","TheButton","TheMin","name","data","heading","isActive","html","text1","text2","input1","width","disabled","reduce","superp","count","hide","redc","props","propty","methods","gm","punch","console","heal","superpunch","submit"],"sourceRoot":"src/components","sources":["ThirdIndex.vue"],"sourcesContent":["<template>\n    <div>\n        <h1>Third Index Title</h1>\n        <div class=\"health\">\n            <div class=\"health-blood\" :style=\"{ 'width': width + '%' }\"></div>\n        </div>\n        <div>\n            <button @click=\"punch\" :disabled=\"disabled\">Punch</button>\n            <button @click=\"superpunch\" :disabled=\"superp\">Super Punch</button>\n            <button @click=\"heal\">Heal</button>\n        </div>\n        <div>\n            <ul v-for=\"x in propty\" :key=\"x\">\n                <li>\n                    {{ x.name }} -- {{ x.id }}\n                </li>\n            </ul>\n            <form action=\"\">\n                <input type=\"text\" ref=\"input\" v-model=\"input1\" value=\"\">\n                <h2>{{ input1 }}</h2>\n                <button @click.prevent=\"submit\">submit</button>\n            </form>\n            <h3 ref=\"hello\" v-if=\"hide\" :class=\"{ hell0: true }\">hello</h3>\n            <button @click=\"hide = !hide\">hide</button>\n        </div>\n        <TheButton btntext=\"submit\" />\n        <TheButton btntext=\"Not submit\" />\n        <div>\n            <button @click=\"$emit('chnageTitle','text is change')\">click me</button>\n        </div>\n        <div>\n            <p>{{text1}}</p>\n            <button @click=\"gm\" :class=\"[isActive ? activeClass : errorClass]\">Gm to Gn</button>\n            <p v-html=\"html\" :class=\"`btn--${width}`\"></p>\n        </div>\n        <TheMin>\n            <a slot=\"link\">TheMin Link</a>\n            <p slot=\"para\" >Lorem ipsum dolor sit amet consectetur adipisicing elit. In, nostrum!</p>\n        </TheMin>\n    </div>\n</template>\n<script>\nimport TheButton from \"./TheButton.vue\";\nimport TheMin from \"./TheMin.vue\";\nexport default {\n    components: {\n        TheButton,\n        TheMin\n    },\n    name: \"ThirdIndex\",\n    data() {\n        return {\n            heading: false,\n            isActive: true,\n            html: \"<p>HTML text here</p>\",\n            text1: \"good morning\",\n            text2: \"good night\",\n            input1: null,\n            width: 100,\n            disabled: false,\n            reduce: 15,\n            superp: true,\n            count: 0,\n            hide: true,\n            redc: false\n        }\n    },\n    props: {\n        propty: Array\n    },\n    methods: {\n        gm() {\n            this.text1= this.text2\n        },\n        punch() {\n            this.count++\n            console.log(this.count)\n            if (this.count % 3 == 0) {\n                this.superp = false\n            }\n            if (this.width >= this.reduce) {\n                this.width -= this.reduce;\n            }\n            else if (this.width <= this.reduce) {\n                this.width = 0;\n                this.disabled = true;\n            }\n        },\n        heal() {\n            this.width = 100\n            if (this.width >= 0) {\n                this.disabled = false\n            }\n        },\n        superpunch() {\n            this.count = 0;\n            this.width = 0;\n            this.superp = true;\n            if (this.width <= this.reduce) {\n                this.width = 0;\n                this.disabled = true;\n            }\n        },\n        submit() {\n            console.log(this.$refs)\n        }\n    },\n}\n</script>\n<style>\n.health {\n    width: 600px;\n    height: 30px;\n    margin: 30px auto;\n    border: 1px solid black;\n}\n\n.health-blood {\n    background-color: red;\n    height: 100%;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}