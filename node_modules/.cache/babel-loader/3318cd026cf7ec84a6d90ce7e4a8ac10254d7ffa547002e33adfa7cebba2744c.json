onsole.log(this.$refs)\n    },\n    computed: {\n        dun() {\n            console.log(\"hello\")\n            return this.a + this.age\n        },\n        dun2() {\n            console.log(\"fello\")\n            return this.b + this.age\n        }\n    }\n}\n</script>\n<style>\n.box {\n    width: 100%;\n    height: 600px;\n    background-color: red;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\nspan {\n    color: white;\n    font-size: 50px;\n}\n\n.red {\n    font-size: 50px;\n    color: red;\n}\n\n.blue {\n    font-size: 50px;\n    color: blue;\n}\n\n.employ-box {\n    display: flex;\n    justify-content: center;\n    gap: 50px;\n}\n\n.employ-box__card {\n    background-color: #5db3ca;\n    padding: 50px;\n    list-style: none;\n}\n\n.employ-name {\n    font-size: 30px;\n    color: white;\n    font-weight: 900;\n    margin-bottom: 30px;\n}\n\n.employ-btn {\n    padding: 10px;\n    background-color: #ffbbff;\n    border-radius: 5px;\n    box-shadow: 0px 0px 5px #636363;\n    border: none;\n}\n\n.employ-name--color {\n    color: purple;\n}\n.orange{\n    background-color: orange;\n    padding: 50px 0;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}