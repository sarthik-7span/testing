un() {\n            return console.log(\"entered\")\n        },\n        troy(a, b, c) {\n            return a + b + c;\n        },\n        \n    },\n    mounted() {\n            console.log(this.$refs.input.focus)\n    },\n    computed: {\n        dun() {\n            console.log(\"hello\")\n            return this.a + this.age\n        },\n        dun2() {\n            console.log(\"fello\")\n            return this.b + this.age\n        }\n    }\n}\n</script>\n<style>\n.box {\n    width: 100%;\n    height: 600px;\n    background-color: red;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\nspan {\n    color: white;\n    font-size: 50px;\n}\n.red{\n    font-size: 50px;\n    color: red;\n}\n.blue {\n    font-size: 50px;\n    color: blue;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}