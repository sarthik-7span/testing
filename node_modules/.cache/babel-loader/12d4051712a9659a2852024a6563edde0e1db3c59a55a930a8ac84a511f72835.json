{"ast":null,"code":"import TheIndex from './components/TheIndex.vue';\nimport TheSub from './components/TheSub.vue';\nimport TheOne from \"./components/TheOne.vue\";\nimport TheTwo from \"./components/TheTwo.vue\";\n// import { component } from 'vue/types/umd';\n\nexport default {\n  name: 'App',\n  components: {\n    TheIndex,\n    TheSub,\n    TheOne,\n    TheTwo\n    // component\n  },\n\n  data() {\n    return {\n      title: \"Ckekc ckeck\",\n      fruit: [{\n        name: \"Mango\",\n        color: \"yellow\"\n      }, {\n        name: \"applr\",\n        color: \"red\"\n      }, {\n        name: \"kiwi\",\n        color: \"green\"\n      }, {\n        name: \"orange\",\n        color: \"orange\"\n      }],\n      head: \"Heading Calling\",\n      fname: \"HEY HEY SAY HEY HEY\",\n      components: \"TheOne\"\n    };\n  },\n  methods: {\n    updateTitle(updatedTitle) {\n      this.head = updatedTitle;\n    },\n    getName(value) {\n      this.fname = value; // Raja Tamil\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA;AACA;AACA;AACA;AACA;;AAEA;EACAA;EACAC;IACAC;IACAC;IACAC;IACAC;IACA;EACA;;EACAC;IACA;MACAC;MACAC,QACA;QAAAR;QAAAS;MAAA,GACA;QAAAT;QAAAS;MAAA,GACA;QAAAT;QAAAS;MAAA,GACA;QAAAT;QAAAS;MAAA,EACA;MACAC;MACAC;MACAV;IACA;EACA;EACAW;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AAEA","names":["name","components","TheIndex","TheSub","TheOne","TheTwo","data","title","fruit","color","head","fname","methods","updateTitle","getName"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <TheIndex v-bind:head=\"head\" gm=\"Good Morning\" v-on:changeTitle=\"updateTitle($event)\" test=\"successfully done\" />\n    <TheSub v-bind:fname=\"fname\" @fname=\"getName\"/>\n    <component v-bind:is=\"components\"/>\n    <button>TheOne</button>\n    <button>TheTwo</button>\n\n  </div>\n</template>\n\n<script>\nimport TheIndex from './components/TheIndex.vue'\nimport TheSub from './components/TheSub.vue'\nimport TheOne from \"./component