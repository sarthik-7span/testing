{"ast":null,"code":"export default {\n  name: \"ThirdIndex\",\n  data() {\n    return {\n      width: 100,\n      disabled: false,\n      reduce: 15,\n      superp: true,\n      count: 0,\n      propty: String\n    };\n  },\n  methods: {\n    punch() {\n      this.count++;\n      console.log(this.count);\n      if (this.count % 3 == 0) {\n        this.superp = false;\n      }\n      if (this.width >= this.reduce) {\n        this.width -= this.reduce;\n      } else if (this.width <= this.reduce) {\n        this.width = 0;\n        this.disabled = true;\n      }\n    },\n    heal() {\n      this.width = 100;\n      if (this.width >= 0) {\n        this.disabled = false;\n      }\n    },\n    superpunch() {\n      this.count = 0;\n      this.width = 0;\n      this.superp = true;\n      if (this.width <= this.reduce) {\n        this.width = 0;\n        this.disabled = true;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAiBA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;MACAC;MACA;QACA;MACA;MACA;QACA;MACA,OACA;QACA;QACA;MACA;IACA;IACAC;MACA;MACA;QACA;MACA;IACA;IACAC;MACA;MACA;MACA;MACA;QACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","width","disabled","reduce","superp","count","propty","methods","punch","console","heal","superpunch"],"sourceRoot":"src/components","sources":["ThirdIndex.vue"],"sourcesContent":["<template>\n    <div>\n        <h1>Third Index Title</h1>\n        <div class=\"health\">\n            <div class=\"health-blood\" :style=\"{ 'width': width + '%' }\"></div>\n        </div>\n        <div>\n            <button @click=\"punch\" :disabled=\"disabled\">Punch</button>\n            <button @click=\"superpunch\" :disabled=\"superp\">Super Punch</button>\n            <button @click=\"heal\">Heal</button>\n        </div>\n        <div>\n            <p>${{ propty }}</p>\n        </div>\n    </div>\n</template>\n<script>\nexport default {\n    name: \"ThirdIndex\",\n    data() {\n        return {\n            width: 100,\n            disabled: false,\n            reduce: 15,\n            superp: true,\n            count: 0,\n            propty: String\n        }\n    },\n    methods: {\n        punch() {\n            this.count++\n            console.log(this.count)\n            if (this.count % 3 == 0) {\n                this.superp = false\n            }\n            if (this.width >= this.reduce) {\n                this.width -= this.reduce;\n            }\n            else if (this.width <= this.reduce) {\n                this.width = 0;\n                this.disabled = true;\n            }\n        },\n        heal() {\n            this.width = 100\n            if (this.width >= 0) {\n                this.disabled = false\n            }\n        },\n        superpunch() {\n            this.count = 0;\n            this.width = 0;\n            this.superp = true;\n            if (this.width <= this.reduce) {\n                this.width = 0;\n                this.disabled = true;\n            }\n        }\n    }\n}\n</script>\n<style>\n.health {\n    width: 600px;\n    height: 30px;\n    margin: 30px auto;\n    border: 1px solid black;\n}\n\n.health-blood {\n    background-color: red;\n    height: 100%;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}